inner join dwhgrupoandrade.reporteindicadores i on e.idindicadores=i.idindicadores 
where i.dwhbase='DWHGRUPOANDRADE' and e.perfil='Admin'
order by i.marca)NUEVOS on MARCAS.marca=NUEVOS.marca
left join 
(SELECT  distinct 
i.marca
,i.nombreimgmarca 
FROM dwhgrupoandrade.enviareportes e
inner join dwhgrupoandrade.reporteindicadores i on e.idindicadores=i.idindicadores 
where i.dwhbase='DWHSEMINUEVOSANDRADE' or i.dwhbase='DWHSEMINUEVOSCAMIONESANDRADE' and e.perfil='Admin'
order by i.marca)SEMINUEVOS on MARCAS.marca=SEMINUEVOS.marca
